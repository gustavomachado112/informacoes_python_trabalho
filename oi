
nome = input("Nome: ")
data_nascimento = input("Data de nascimento (DD/MM/AAAA): ")
telefone = input("Telefone (apenas números): ")
endereco = input("Endereço: ")
cpf = input("CPF (apenas 11 números): ")


if len(nome.strip()) == 0:
    print("Nome não pode estar vazio!")

if len(data_nascimento) != 10 or data_nascimento[2] != '/' or data_nascimento[5] != '/':
    print("Data deve estar no formato DD/MM/AAAA!")

if not telefone.isdigit() or len(telefone) < 10:
    print("Telefone deve ter pelo menos 10 números!")

if len(endereco.strip()) <= 5:
    print("Endereço deve ter mais de 5 caracteres!")

if not cpf.isdigit() or len(cpf) != 11:
    print("CPF deve ter exatamente 11 números!")
